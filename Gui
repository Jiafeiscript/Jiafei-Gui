-- Create the Camlock button
local camlockButton = Instance.new("TextButton")
camlockButton.Size = UDim2.new(0.5, 0, 0.1, 0)
camlockButton.Position = UDim2.new(0.25, 0, 0.2, 0)
camlockButton.BackgroundColor3 = Color3.fromRGB(255, 182, 193) -- Red color
camlockButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White color for text
camlockButton.Text = "Resolver ðŸ˜²ðŸ¥³"
camlockButton.Parent = frame

-- Connect the Camlock button to the toggle function
camlockButton.MouseButton1Click:Connect(toggleCamlock)

-- Create the Close button
local closeButton = Instance.new("TextButton")
closeButton.Size = UDim2.new(0.2, 0, 0.1, 0)
closeButton.Position = UDim2.new(0.8, 0, 0, 0)
closeButton.BackgroundColor3 = Color3.fromRGB(255, 182, 193) -- Red color
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White color for text
closeButton.Text = "Close ðŸ¥°"
closeButton.Parent = frame

-- Function to close the GUI
local function closeGUI()
    gui:Destroy()
end

-- Connect the Close button to the close function
closeButton.MouseButton1Click:Connect(closeGUI) 
